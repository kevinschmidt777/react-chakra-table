{
  "name": "react-chakra-table",
  "version": "0.9.7",
  "description": "a simple, sortable, filterable, react table grid - styled with chakra-ui.",
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json"
  },
  "keywords": [
    "chakra",
    "chakra-ui",
    "react-table",
    "react-grid",
    "table",
    "table-grid",
    "data-grid",
    "grid"
  ],
  "author": "Kevin Schmidt",
  "license": "ISC",
  "files": [
    "lib/**/*"
  ],
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/kevinschmidt777/react-chakra-table/issues"
  },
  "homepage": "https://github.com/kevinschmidt777/react-chakra-table#readme",
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "prettier": "^2.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "@chakra-ui/react": ">=2",
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "framer-motion": ">=6"
  },
  "dependencies": {
    "@tanstack/react-table": "8.7.4",
    "dayjs": "1.11.7",
    "react-export-table-to-excel": "1.0.6"
  }
}
